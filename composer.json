{
    "name": "modelthemesnippets\/nft-marketplace-core-lite",
    "description": "Light version of a NFT Marketplace in WordPress has never been this easy.",
    "license": "GPL-3.0-or-later",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "ModelTheme",
            "email": "support@modeltheme.com",
            "homepage": "https:\/\/modeltheme.com"
        }
    ],
    "homepage": "https://modeltheme.com/enefti-lite-vs-enefti-core/",
    "require": {
        "php": ">=7.0",
        "drlecks/simple-web3-php": "^0.9.0",
        "krowinski/bcmath-extended": "dev-master",
        "ayecode\/wp-super-duper": "2.0.2",
        "cmb2\/cmb2": "2.9.*",
        "composer\/installers": "1.x",
        "decodelabs\/tagged": "^0.11.9",
        "freemius\/wordpress-sdk": "2.4.*",
        "inpsyde\/assets": "^2.8",
        "inpsyde\/wp-context": "1.3.*",
        "johnbillion\/extended-cpts": "4.5.*",
        "julien731\/wp-review-me": "dev-develop",
        "micropackage\/requirements": "^1.0",
        "origgami\/cmb2-grid": "dev-master",
        "seravo\/wp-custom-bulk-actions": "dev-master",
        "stevegrunwell\/wp-cache-remember": "dev-master",
        "wpbp\/cpt_columns": "1.0.*",
        "wpbp\/cronplus": "1.0.*",
        "wpbp\/debug": "1.1.*",
        "wpbp\/page-madness-detector": "^1.0",
        "wpbp\/pointerplus": "1.0.*",
        "wpbp\/template": "1.0.*",
        "wpbp\/widgets-helper": "1.0.*",
        "wpdesk\/wp-notice": "^3.2",
        "yahnis-elsts\/plugin-update-checker": "^4.9",
        "yoast\/i18n-module": "3.1.*"
    },
    "require-dev": {
        "codeatcode\/codeatcs": "^1.0",
        "codeception\/codeception": "4.2.*",
        "codeception\/module-asserts": "^1.0",
        "codeception\/module-cli": "^1.0",
        "codeception\/module-db": "^1.0",
        "codeception\/module-filesystem": "^1.0",
        "codeception\/module-phpbrowser": "^1.0",
        "codeception\/module-rest": "^1.2",
        "codeception\/module-webdriver": "^1.0",
        "codeception\/util-universalframework": "^1.0",
        "dealerdirect\/phpcodesniffer-composer-installer": "^0.7",
        "ergebnis\/composer-normalize": "^2.6",
        "lucatume\/function-mocker": "~1.0",
        "lucatume\/wp-browser": "^3.1",
        "phpro\/grumphp": "^1.13",
        "phpunit\/phpunit": "^9.2",
        "szepeviktor\/phpstan-wordpress": "^1.1.2"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https:\/\/github.com\/ayecode\/wp-super-duper"
        },
        {
            "type": "package",
            "package": {
                "name": "mte90\/i18n-module",
                "version": "3.1.0",
                "autoload": {
                    "classmap": [
                        "src\/"
                    ]
                },
                "source": {
                    "type": "git",
                    "url": "https:\/\/github.com\/mte90\/i18n-module.git",
                    "reference": "master"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "NFT_Marketplace_Core\\Backend\\": "backend\/",
            "NFT_Marketplace_Core\\Backend\\ActDeact\\": "backend\/act-deact\/",
            "NFT_Marketplace_Core\\Frontend\\": "frontend\/",
            "NFT_Marketplace_Core\\Rest\\": "rest\/",
            "NFT_Marketplace_Core\\Ajax\\": "ajax\/",
            "NFT_Marketplace_Core\\Internals\\": "internals\/",
            "NFT_Marketplace_Core\\Cli\\": "cli\/",
            "NFT_Marketplace_Core\\Integrations\\": "integrations\/",
            "NFT_Marketplace_Core\\Engine\\": "engine\/",
            "NFT_Marketplace_Core\\Internals\\TaxMeta\\": "internals\/tax-meta\/"
        },
        "files": [
            "vendor\/wp-custom-bulk-actions\/custom-bulk-actions.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer\/installers": true,
            "cweagans\/composer-patches": true,
            "dealerdirect\/phpcodesniffer-composer-installer": true,
            "ergebnis\/composer-normalize": true,
            "phpro\/grumphp": true
        }
    },
    "extra": {
        "installer-paths": {
            "vendor\/{$name}\/": [
                "cmb2\/cmb2",
                "origgami\/cmb2-grid",
                "stevegrunwell\/wp-cache-remember",
                "seravo\/wp-custom-bulk-actions"
            ]
        },
        "patches": {
            "ayecode\/wp-super-duper": {
                "Fix v2": "https:\/\/patch-diff.githubusercontent.com\/raw\/AyeCode\/wp-super-duper\/pull\/25.patch"
            }
        }
    }
}
